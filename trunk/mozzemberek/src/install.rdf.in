	<Description>
    	    <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
    	    <em:minVersion>@TB_MIN@</em:minVersion>
    	    <em:maxVersion>@TB_MAX@</em:maxVersion>
    	</Description>
    </em:targetApplication>
    
    <em:targetApplication>
        <!-- Mozilla SeaMonkey -->
	<Description>
	    <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
	    <em:minVersion>@SM_MIN@</em:minVersion>
	    <em:maxVersion>@SM_MAX@</em:maxVersion>
	</Description>
    </em:targetApplication>

 </Description>
